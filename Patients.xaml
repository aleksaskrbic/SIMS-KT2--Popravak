        <Button Content="View all patients" Click=" ViewAllP" HorizontalAlignment="Left" Margin="548.2,319,0,0" VerticalAlignment="Top" Width="120" Grid.Column="1"/>
        <Button Content="Add Patient" Grid.Column="1" HorizontalAlignment="Left" Margin="139.2,319,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
      
    </Grid>
</Window>
